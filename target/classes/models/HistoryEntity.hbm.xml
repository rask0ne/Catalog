<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="models.HistoryEntity" table="history" schema="catalogdb">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="username">
            <column name="username" sql-type="varchar(128)"/>
        </property>
        <property name="downloadDate">
            <column name="downloadDate" sql-type="date"/>
        </property>
        <property name="filesize">
            <column name="fileSize" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>